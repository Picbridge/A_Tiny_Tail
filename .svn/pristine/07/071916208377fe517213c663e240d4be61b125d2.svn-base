using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class CheckWin_GoalLine : MonoBehaviour
 {
    public GameObject gameSceneManager;

    private void OnTriggerEnter(Collider other)
    {
        if (other.tag == "Player")
        {
            if (GameSceneIndexManager.instance.currentLevelIndex + 1 == TileMapManager.instance.levelSize)
                gameSceneManager.GetComponent<GameSceneManager>().PopupWinAllLevelPanel();
            else
                gameSceneManager.GetComponent<GameSceneManager>().PopUpWinningPanel();

        }
    }
}
